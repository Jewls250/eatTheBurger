<div class="container">

    <div class="row text-center">
        <h1>Hello</h1>
    </div>

    <div class="row">
        <div class="col">
            <h2>Waiting</h2>
            <ul>
            {{#each burger}}
            {{#unless this.devoured}}
                <li>{{this.burger_name}}</li>
            {{/unless}}
            {{/each}}
            </ul>
        </div>
        <div class="col">
            <h2>Devoured</h2>
            <ul>
                {{#each burger}}
                {{#if devoured}}
                <li>{{this.burger_name}}</li>
                {{/if}}
                {{/each}}
            </ul>
        </div>
    </div>
    <div class="row">
        <input type="text" id="userBurger">
        <button id="submitB">Submit</button>
    </div>


</div>




